                      "format": "uuid-v4",
                      "example": "6d814c28-fe73-443c-bfe0-48d518aa32fa"
                    },
                    "status": {
                      "description": "Credit status",
                      "type": "string",
                      "example": "CREATED"
                    },
                    "value": {
                      "description": "Credit value",
                      "type": "number",
                      "example": 10.12
                    },
                    "history_code": {
                      "description": "The credit history code",
                      "type": "string",
                      "enum": [
